<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/listagem"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Listagem">

    <TextView
        android:id="@+id/tvLista"
        android:layout_width="200dp"
        android:layout_height="38dp"
        android:layout_marginTop="32dp"
        android:background="#350106"
        android:gravity="center"
        android:text="@string/tvLista"
        android:textAlignment="center"
        android:textColor="#fff"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/lvAutomoveis"
        android:layout_width="365dp"
        android:layout_height="650dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvLista"
        app:layout_constraintVertical_bias="0.361" />

    <Button
        android:id="@+id/btnVoltar"
        android:layout_width="85dp"
        android:layout_height="50dp"
        android:layout_marginTop="26dp"
        android:backgroundTint="@color/black"
        android:text="@string/btnVoltar"
        android:textSize="11sp"
        app:layout_constraintEnd_toStartOf="@+id/tvLista"
        app:layout_constraintHorizontal_bias="0.592"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>